module T01 where

/**
 * docstrings on submodules get run inside the scope of the submodule
 *
 * ```repl
 * :exhaust pp && pp
 * ```
 */
submodule Sub where
  /**
   * This verifies that pp shadows the outer definition
   * ```repl
   * :check pp
   * ```
   */
  pp = True

/**
 * ```repl
 * :check ~ pp
 * ```
 */
pp = False
